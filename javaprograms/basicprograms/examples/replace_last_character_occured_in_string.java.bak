class replace_last_character_occured_in_string 
{
	public static void main(String[] args) 
	{
		String str="nithin";
		int n=str.length()-1;
		String str1="";
		char c=str.charAt(n);
		for (int i=0;i<=n ;i++ )
		{
			char ch=str.charAt(i);
			if (ch==c)
			{
				str1=str1+'u';
			}
			else
			{
				str1=str1+ch;
			}
		}
		System.out.println(str1);
	}
}
