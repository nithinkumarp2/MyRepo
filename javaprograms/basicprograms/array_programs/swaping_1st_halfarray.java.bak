class swaping_1st_halfarray 
{
	public static void main(String[] args) 
	{
		int[] a={1,8,39,32,8,35,4,38,41};
		int n=a.length-1;
		for (int i=0;i<=n ;i++ )
		{
			for (int j=0;j<n/2-1 ;j++ )
			{
				if(a[j]>a[j+1])
				{
				int ele=a[j];
				a[j]=a[j+1];
				a[j+1]=ele;
				}
			}
			System.out.println(a[i]);
		}
		
		
	}
}
