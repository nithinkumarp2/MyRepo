class min_value_for_all_even_index 
{
	public static void main(String[] args) 
	{
		int a[]={1,2,7,3,6,4};
		int n=a.length-1;
		int b[]=new int[n+1];
		int min=a[0];
		for (int i=0;i<=n ;i++ )
		{
			if (i%2==0&&i!=0)
			{
				
				 if (min<a[i])
				 {
					 min=a[i];
				 }

				}
		}
		for (int i=0;i<=n ;i++ )
		{
		System.out.println(min+"="+a[i]);
		}
	}
}
