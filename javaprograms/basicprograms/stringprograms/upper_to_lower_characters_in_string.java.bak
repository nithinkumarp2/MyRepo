class upper_to_lower_characters_in_string  
{
	public static void main(String[] args) 
	{
		 String str="NITHIN kumar";
		 int n=str.length()-1;
		 String str1="";
		 for (int i=0;i<=n ;i++ )
		 {
			 char ch=str.charAt(i);
			 if(ch>='A'&&ch<='Z')
			 {
			 ch+=32;
			 str1=str1+ch;
		 }
		 else
			 {
			 str1=str1+ch;
			 }
		 }
		 System.out.println(str1);
	}
}
