class lower_to_upper_characters_in_string 
{
	public static void main(String[] args) 
	{
		String str="Nithin";
		String str1="";
		int n=str.length()-1;
		for (int i=0; i<=n;i++ )
		{
		
			char ch=str.charAt(i);

                   ch-=32;
				   str1=str1+ch;
		}
		System.out.println(str1);
	}
}
