import java.util.Scanner;
class smalleven
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("enter m value");
		int n=sc.nextInt();
			if (n%2==0)
			{
			   int n1=n;
			   int smallest =n1;
			   while(n1!=0)
				{
				   int digit=n1%10;
				   if(smallest>digit)
					{
					   smallest=digit;
					}
					n=n/10;
					break;
					System.out.println(smallest);

		         }
				 
	}
}
}
