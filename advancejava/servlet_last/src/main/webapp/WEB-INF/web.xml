<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>insert</servlet-name>
		<servlet-class>com.jsp.demo.insersation</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>insert</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>details</servlet-name>
		<servlet-class>com.jsp.demo1.select</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>details</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>update_details</servlet-name>
		<servlet-class>com.jsp.demo1.update</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>update_details</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>delete_details</servlet-name>
		<servlet-class>com.jsp.demo1.delete</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>delete_details</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>
